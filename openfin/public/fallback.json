{
  "devtools_port": 9090,
  "startup_app": {
    "name": "OpenFinAppExample",
    "description": "A simple OpenFin application example",
    "url": "http://localhost:3000/analyser",
    "uuid": "OpenFinAppExample",
    "licenseKey": "test123",
    "defaultTop": 50,
    "defaultLeft": 50,
    "autoShow": true,
    "defaultWidth": 1024,
    "defaultHeight": 768,
    "defaultCentered": true,
    "resizable": true,
    "frame": true,
    "customData": {
      "toolbar": true,
      "toolbarSettings": {
        "icon": "http://localhost:3000/path/to/icon.png",
        "items": [
          {
            "label": "Home",
            "icon": "http://localhost:3000/path/to/home-icon.png",
            "action": "navigate",
            "url": "http://localhost:3000/home"
          },
          {
            "label": "Refresh",
            "icon": "http://localhost:3000/path/to/refresh-icon.png",
            "action": "refresh"
          }
        ]
      }
    },
    "permissions": {
      "System": {
        "launchExternalProcess": true,
        "openUrlWithBrowser": true,
        "registerCustomProtocol": true,
        "unregisterCustomProtocol": true,
        "getCustomProtocolState": true,
        "terminateExternalProcess": true
      },
      "webAPIs": ["notifications", "audio", "video"]
    }
  },
  "runtime": {
    "arguments": "",
    "version": "38.126.82.64"
  },
  "shortcut": {
    "company": "Your Company",
    "description": "OpenFin Application Example",
    "icon": "http://localhost:3000/path/to/your/icon.png",
    "name": "OpenFinAppExample"
  }
}
